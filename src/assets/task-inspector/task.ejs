<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd><%= marked(metadata.name) %></dd>

  <dt>Description</dt>
  <dd><%= marked(metadata.description) %></dd>

  <dt>Owner</dt>
  <dd><code><%= metadata.owner %></code></dd>
</dl>

<dl class="dl-horizontal">
  <dt>Created</dt>
  <dd><%= moment(created).fromNow() %></dd>

  <dt>Deadline</dt>
  <dd>
      <%= moment(deadline).fromNow() %>
      (<%= moment(deadline).from(created, true) %> later)
  </dd>
</dl>

<dl class="dl-horizontal">
  <dt>Provisioner-id</dt>
  <dd><code><%= provisionerId %></code></dd>
  <dt>Worker-type</dt>
  <dd><code><%= workerType %></code></dd>
</dl>

<dl class="dl-horizontal">
  <dt>Payload</dt>
  <dd>
    <pre class="language-javascript"><code><%=
      JSON.stringify(payload, undefined, 2)
    %></code></pre>
  </dd>
</dl>